{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kafka": {
    "BootstrapServers": [ "kafka1:9092", "kafka2:9093", "kafka3:9094" ],
    "Topic": "my-topic",
    "GroupId": "my-group"
  },
  "RedisOptionsDeveloping": {
    "Endpoints": [ "redis_container:6379" ],
    "AbortOnConnectFail": false,
    "SyncTimeout": 5000
  },
  "RedisOptionsTesting": {
    "Endpoints": [ "localhost:6379" ],
    "AbortOnConnectFail": false,
    "SyncTimeout": 5000
  },
  "WebApiOutboxBaseUrl": "https://localhost:5002/"
}
